SELECT * FROM livros//SELECIONAR EM ORDEM ALFABETICA//
ORDER BY TITULO, AUTOR, DATALAN;

SELECT DATALAN FROM livros WHERE DATALAN = 2011 OR DATALAN = 2012;//teste n ta pronto//
