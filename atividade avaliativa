CREATE TABLE Assuntos(
ID INT(3) AUTO_INCREMENT PRIMARY KEY NOT NULL,
NOME CHAR(30) NOT NULL
);

CREATE TABLE Livros(
ID INT(6) AUTO_INCREMENT PRIMARY KEY NOT NULL,
TITULO CHAR(50),
AUTOR CHAR(50),
ASSUNTO_ID INT(3) NOT NULL,
DATALAN DATE,
COPIAS INT(3)
);

CREATE TABLE Emprestimo(
ID int(8) AUTO_INCREMENT PRIMARY KEY NOT NULL,
ALUNO_ID INT(5),
 LIVRO_ID INT(6),   
 DATARET DATE,
 DATAPREV DATE,
 DATADEV DATE,
 MULTA FLOAT(6,2)
);


CREATE TABLE Alunos(
    ID INT(5) AUTO_INCREMENT PRIMARY KEY NOT NULL,
    NOME CHAR(50),
    EMAIL CHAR(50),
    FONE CHAR(20)
);

